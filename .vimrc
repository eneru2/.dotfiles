set number
set relativenumber

set expandtab
set shiftwidth=2
set tabstop=2

set autoindent
set smartindent

set encoding=utf-8
set updatetime=300
set autochdir
"set wrap lbr 
" Status bar
set laststatus=2

" keybinds config
source ~/.vim/config/keybinds.vim

" plug plugins config
source ~/.vim/config/plug.vim

" coc config
source ~/.vim/config/coc.vim

" onedark theme config
source ~/.vim/config/onedarktheme.vim
